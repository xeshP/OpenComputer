component = require("component")
compressor = component.advanced_air_compressor
transposer1 = "a08f3aa3-e4a4-45d8-ab10-14eca5aab9a4"
compressor1 = "91246d04-ee67-4bb8-a4bd-c0a88baf1af4"
inv = component.proxy(transposer1)
compressor = component.proxy(compressor1)

print(compressor.getPressure())

slot = 1
invstack = true
while invstack == true do
    if compressor.getPressure() > 4 then
        
        os.sleep(5)
    else
        print("Under 4")
        if inv.getSlotStackSize(3, slot) == 0 then
            slot = slot+1
            if slot > inv.getInventorySize(3) then
                invstack = false
                print("Nothin in inv found!11")
            end
        else
            coalstack = inv.getSlotStackSize(2, 1)
            if coalstack < 2 then
                inv.transferItem(3, 2, 2, slot, 1)
                os.sleep(5)
            end
                
        end

    end
end
