side = 3   ----transposer side to chest or tank
component = require("component")
inv = component.transposer
serial=require"serialization"
 
filer=io.open("listo","r")
info=filer:read("*all")
filer:close()
 
nr = serial.unserialize(info)
print(#nr)
 
print("chest or tank")
inpst = io.read()
if inpst == "tank" then
    chest = component.ender_tank
    choice = "tank"
    else
        chest = component.ender_chest
        choice = "chest"
        end
 
 
rnd = 0
for id = 1,#nr do
    print(nr[id].."                     ["..id.."]")
    for word in string.gmatch(nr[id], '([^,]+)') do
        rnd = rnd+1
        if rnd == 1 then
            one = tonumber(word)
        else if rnd == 2 then
            two = tonumber(word)
        else if rnd == 3 then
            three = tonumber(word)
            rnd = 0
            end
        end
        end
    end
 
 
    chest.setFrequency(one,two,three)
    if choice == "tank" then
        invo = inv.getTankLevel(side)
        if invo > 0 then
            print(one.."!"..two.."!"..three)
            print("fluid tank found!!!")
            io.read()
            end
 
        else
            if inpst == "chest fast" then
                maxsize = 2
                else
                    maxsize = 27
                    end
                    stacks = inv.getAllStacks(side)
                    for stack in stacks do
                        if stack.name ~= "minecraft:air" and stack.name ~= nil then
                            print(stack.name)
                            print(one.."!"..two.."!"..three)
                            print("ITEM FOUND!!1$$$")
                            io.read()
                        end
                    end
                    
 
            end
 
    end
 
 
 
