component = require("component")
sensor = component.universalsensor
playerlist = {"tertton", "LetsJustDoThis", "KGvMashiro", "KGvAsuna"}



while true do
    
for idr = 1, #playerlist do
    sensor.setTextfield(playerlist[idr])
    os.sleep(1)
    if sensor.getSensorValue() == 1 then
        print("found "..playerlist[idr])
    end

end

end

